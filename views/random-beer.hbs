<h1>Random Beer</h1>

{{#each beer}} 
	<div class='random-beer-details'>
    <img src='{{image_url}}' alt='{{name}}' />
    <h2>{{this.name}}</h2>
    <p>{{this.description}}</p>
    <p>{{this.tagline}}</p>
		<ul>
			{{#each food_pairing}}
		    <li>{{this}}</li>
			{{/each}}
		</ul>
    <p>{{this.brewers_tips}}</p>
  </div>
{{/each}}